{
  "$schema": "https://json.schemastore.org/angularcli",
  "version": 1,
  "projects": {
    "profilr-frontend": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/profilr-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "css",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": { "browserTarget": "profilr-frontend:build" },
          "configurations": {
            "production": { "browserTarget": "profilr-frontend:build:production" }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
